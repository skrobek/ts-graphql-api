{
  "type": "postgres",
  "url": "postgres://tsapi:tsapi@localhost/tsapi",
  "synchronize": true,
  "logging": false,
  "cli": {
    "entitiesDir": "src/models",
    "migrationsDir": "src/migration"
  },
  "entities": [
    "src/models/**/*.ts"
  ],
  "migrations": [
    "src/migration/**/*.ts"
  ],
  "subscribers": [
    "src/subscriber/**/*.ts"
  ]
}
